{
  "name": "svelte-pocketbase-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev:sveltekit": "pnpm run typegen && vite dev",
    "dev:pocketbase": "cd pocketbase && ./pocketbase serve",
    "dev": "pnpm run '/^dev:.*/'",
    "build": "pnpm run typegen && pnpm check && vite build",
    "preview": "vite preview",
    "typegen": "pocketbase-typegen --db ./pocketbase/pb_data/data.db --out src/lib/pocketbase-types.ts",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.4",
    "@sveltejs/kit": "^2.5.26",
    "@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
    "@types/eslint": "^9.6.1",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.10",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.43.0",
    "globals": "^15.9.0",
    "pocketbase": "^0.21.5",
    "pocketbase-typegen": "^1.2.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "svelte": "5.0.0-next.244",
    "svelte-check": "^3.8.6",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "vite": "^5.4.3"
  },
  "type": "module"
}
